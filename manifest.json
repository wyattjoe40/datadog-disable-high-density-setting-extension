{
    "manifest_version": 3,
    "name": "DataDog Automatic High-Density Setting Disabler",
    "description": "An extension that automatically disables the high-density setting on DataDog Dashboards.",
    "version": "0.2",
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://app.datadoghq.com/dashboard/*"
        ]
      }
    ],
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }